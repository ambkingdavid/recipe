<section class="w-full ">
        <ul class="flex flex-col space-x-5 space-y-5 lg:flex-wrap lg:flex-row p-5">
            <% for( let i = 0; i < continent.length; i++ ) { %>
                <li class="flex flex-col shadow-lg rounded-lg">
                    <img src="continents[i].image" alt="" class="rounded-lg transition ease-in-out hover:scale-105">
                    <p class="text-blue-500"><%= continents[i].name %></p>
                </li>
            <% } %>
        </ul>
</section>